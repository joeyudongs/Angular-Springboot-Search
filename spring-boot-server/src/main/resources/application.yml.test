server:
  port: 8080

spring:
  datasource:
    url: jdbc:postgresql://localhost:5432/userlab
    username: userlab
    password: userlab
  jpa:
    hibernate:
      ddl-auto: none
    properties:
      hibernate:
        format_sql: true
  sql:
    init:
      mode: always
      schema-locations: classpath:schema.sql
      data-locations: classpath:data.sql
